<h1>Modelos de Carros</h1>

<table>
  <thead>
    <tr>
      <th>Fabricante</th>
      <th>Nome</th>
      <th>Ano</th>
    </tr>
  </thead>

    <% @car_models.each do |cm| %>
  <tbody>
    <tr>
      <td><%= cm.manufacturer.name %></td>
      <td><%= cm.name %></td>
      <td><%= cm.year %></td>
    </tr>
    <% end %>
  </tbody>
</table>
